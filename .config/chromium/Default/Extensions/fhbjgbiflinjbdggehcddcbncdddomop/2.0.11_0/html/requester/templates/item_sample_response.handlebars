<div class="sample-response-container clearfix" data-id="{{id}}">
	<div class="sample-response-name-container">
		<a class="sample-response-name sample-response-actions-load" data-id="{{id}}">{{name}}</a>
	</div>
	{{#ifEditable write}}
	<div class="sample-response-actions">
		<a rel="tooltip" data-original-title="Delete"
		   data-placement="bottom" data-toggle="modal"
		   class="sample-response-actions-delete" data-id="{{id}}">
		    <span class="icon-delete"></span></a>
	</div>
	{{/ifEditable}}
</div>