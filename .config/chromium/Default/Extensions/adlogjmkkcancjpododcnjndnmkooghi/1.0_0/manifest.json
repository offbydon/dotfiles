{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon.png"
   },
   "content_scripts": [ {
      "js": [ "app.js" ],
      "matches": [ "*://*/*" ]
   } ],
   "description": "Checks if websites have weak SHA-1 SSL certificates.",
   "key": "",
   "manifest_version": 2,
   "name": "Check SSL",
   "permissions": [ "https://*/", "http://*/", "https://shaaaaaaaaaaaaa.com/api/check/*", "tabs" ],
   "short_name": "Check SSL",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0",
   "web_accessible_resources": [ "https://shaaaaaaaaaaaaa.com/api/check/*" ]
}
